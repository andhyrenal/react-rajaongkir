{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/proxy.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "destination": "/index.html",
            "statusCode": 200
        }
    ]
}